{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "truck-assist": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/truck-assist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/.htaccess",
              {
                "input": "./node_modules/@google/markerclustererplus/images",
                "glob": "*",
                "output": "/images"
              }
            ],
            "styles": [
              "node_modules/@progress/kendo-theme-material/dist/all.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/pace-progressbar/themes/blue/pace-theme-minimal.css",
              "node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "node_modules/snazzy-info-window/dist/snazzy-info-window.css",
              "node_modules/croppie/croppie.css",
              "node_modules/ol/ol.css",
              "./node_modules/@videogular/ngx-videogular/fonts/videogular.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/colresizable/colResizable-1.6.min.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/scripts/magicline.js",
              "src/assets/scripts/geoxml3.js"
            ],
            "allowedCommonJsDependencies": [
              "hammerjs",
              "ngx-google-places-autocomplete",
              "@agm/snazzy-info-window",
              "snazzy-info-window",
              "src/app/repairs/maintenance-manage/maintenance-manage.component",
              "src/app/load/manage-load/manage-load.component",
              "axios",
              "rxjs/operators",
              "rxjs/Subscription",
              "rxjs/Subject",
              "croppie"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "truck-assist:build",
            "proxyConfig": "./proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "truck-assist:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "truck-assist:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/scripts/geoxml3.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "truck-assist:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "truck-assist:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "truck-assist",
  "cli": {
    "analytics": false,
    "defaultCollection": "@ngrx/schematics"
  }
}
